{
  "name": "io-ts-parser-types",
  "version": "2.4.0",
  "description": "io-ts types for making parsers",
  "main": "build/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "lint": "eslint .",
    "release": "npm run test && npm run lint && npm run build && release-it",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "codec",
    "io-ts",
    "typescript",
    "types",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/velocityzen/io-ts-parser-types.git"
  },
  "bugs": {
    "url": "https://github.com/velocityzen/io-ts-parser-types/issues"
  },
  "homepage": "https://github.com/velocityzen/io-ts-parser-types#readme",
  "author": "Alexey Novikov <v@2dubs.com>",
  "license": "MIT",
  "peerDependencies": {
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/eslint__js": "^8.42.3",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@type-challenges/utils": "^0.1.1",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "io-ts-types": "^0.5.19",
    "prettier": "^3.0.1",
    "release-it": "^18.0.0",
    "rimraf": "^6.0.0",
    "rollup": "^4.8.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.2.0",
    "@vitest/coverage-v8": "^3.0.7",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "decimal.js": "^10.4.3"
  }
}
