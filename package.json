{
  "name": "io-ts-parser-types",
  "version": "2.0.0",
  "description": "io-ts types for making parsers",
  "main": "build/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "lint": "eslint .",
    "release": "npm run test && npm run lint && npm run build && release-it",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "NODE_ENV=test jest --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watchAll"
  },
  "keywords": [
    "codec",
    "io-ts",
    "typescript",
    "types",
    "parser"
  ],
  "author": "Alexey Novikov <v@2dubs.com>",
  "license": "MIT",
  "peerDependencies": {
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@type-challenges/utils": "^0.1.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "io-ts-types": "^0.5.19",
    "jest": "^29.0.0",
    "prettier": "^3.0.1",
    "release-it": "^16.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.8.0",
    "rollup-plugin-node-externals": "^6.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
